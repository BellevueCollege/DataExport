<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="foo" type="Test.DataExport.WS.FooConfigHandler, Test.DataExport.WS" />
    <section name="dataExport" type="DataExport.WS.Config.DataExportConfigHandler, DataExport.WebService" />
  </configSections>
	<connectionStrings>
		<add name="Default" connectionString=""/>
	</connectionStrings>
  <appSettings>
    <add key="SftpDestinationPath" value=""/>
    <add key="SftpHostname" value=""/>
    <add key="SftpLogin" value=""/>
    <add key="MaxientSftpKeyFile" value=""/>
    <add key="MaxientSftpHostname" value=""/>
    <add key="MaxientSftpDestinationPathAndFile" value=""/>
    <add key="MaxientSftpLogin" value=""/>
  </appSettings>
  <dataExport>
    <exporters>
      <exporter name="maxient1">
        <sqlInput command="SELECT * FROM vw_Maxient_Feed1" />
        <csvFormat fieldSeparator="|" trimFromFieldStart=", " trimFromFieldEnd=", " />
        <sftpDelivery host="" username="" keyFile="SupportFiles/testkey.rsa" destination="" saveCopy="false" />
      </exporter>
      <exporter name="maxient2">
        <sqlInput command="SELECT * FROM vw_Maxient_Feed2 ORDER BY [SID], CourseID" />
        <xslFormat templateFile="Maxient2.StudentSchedule.xslt" />
        <sftpDelivery host="" username="" keyFile="SupportFiles/testkey.rsa" destination="" saveCopy="false" />
      </exporter>
    </exporters>
  </dataExport>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Common.Logging" publicKeyToken="af08829b84f0328e" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.1.2.0" newVersion="2.1.2.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>